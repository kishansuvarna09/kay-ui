{
  "name": "@kay-ui/core",
  "version": "0.1.0",
  "description": "Core UI components with hybrid MUI approach",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*",
      "require": "./dist/*",
      "types": "./dist/*"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@mui/material": "^5.15.0",
    "@mui/base": "^5.0.0-beta.40",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vitest": "^1.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  }
}